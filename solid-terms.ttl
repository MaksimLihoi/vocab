@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix dcterms: <http://purl.org/dc/terms/issued> .

<>
    a <http://www.w3.org/2002/07/owl#Ontology> ;
    rdfs:label "Solid terms"@en ;
    dcterms:issues "2015-11-16"^^<http://www.w3.org/2001/XMLSchema#date> ;
    dcterms:modified "2016-07-25"^^<http://www.w3.org/2001/XMLSchema#date> .

<#Inbox>
    a rdfs:Class ;
    rdfs:isDefinedBy <> ;
    rdfs:label "Inbox"@en ;
    rdfs:comment "A resource containing notifications."@en .

<#Notification>
    a rdfs:Class ;
    rdfs:isDefinedBy <> ;
    rdfs:label "Notification"@en ;
    rdfs:comment "A notification resource."@en .

<#Timeline>
    a rdfs:Class ;
    rdfs:isDefinedBy <> ;
    rdfs:comment "A resource containing time ordered items and sub-containers.  Sub-containers may be desirable in file based systems to split the timeline into logical components e.g. /yyyy-mm-dd/ as used in ISO 8061."@en ;
    rdfs:label "Timeline"@en .

<#TypeIndex>
    a rdfs:Class ;
    rdfs:isDefinedBy <> ;
    rdfs:label "Type index"@en ;
    rdfs:comment "A index of type registries for resources. Applications can register the RDF type they use and list them in the index resource."@en .
    
<#inbox>
    a rdf:Property ;
    rdfs:isDefinedBy <> ;
    rdfs:label "inbox"@en ;
    rdfs:comment "Used to discover an Inbox to send notifications to and consume from. Used in Linked Data Notifications: http://www.w3.org/TR/ldn/"@en .

<#notification>
    a rdf:Property ;
    rdfs:comment "Notification resource for an inbox."@en ;
    rdfs:isDefinedBy <> ;
    rdfs:label "notification"@en .

<#read>
    a rdf:Property ;
    rdfs:isDefinedBy <> ;
    rdfs:label "read"@en ;
    rdfs:comment "Indicates if a message has been read or not."@en ;
    rdfs:range xsd:boolean .

<#timeline>
    a rdf:Property ;
    rdfs:comment "Timeline for a given resource."@en ;
    rdfs:isDefinedBy <> ;
    rdfs:label "timeline"@en .

<#typeIndex>
    a rdf:Property ;
    rdfs:isDefinedBy <> ;
    rdfs:label "type index"@en ;
    rdfs:comment "Points to a TypeIndex resource."@en ;
    rdfs:range <#TypeIndex> .
